{
  "name": "sator-web-backend",
  "module": "index.ts",
  "type": "module",
  "scripts": {
    "dev": "bun run --watch src/index.ts",
    "build": "tsc",
    "start": "bun run src/index.ts",
    "inspect": "bun run --inspect src/index.ts",
    "studio": "bunx drizzle-kit studio",
    "db:push": "bunx drizzle-kit push",
    "db:gen": "bunx drizzle-kit generate",
    "db:migrate": "bunx drizzle-kit migrate",
    "db:reset": "bun run src/db/reset.ts",
    "db:seed": "bun run src/db/seed.ts",
    "lint": "npm run lint:js",
    "lint:eslint": "eslint --ignore-pattern .gitignore",
    "lint:js": "npm run lint:eslint src/",
    "lint:fix": "npm run lint:js -- --fix",
    "test": "vitest",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "prepare": "husky"
  },
  "prisma": {
    "seed": "npx tsx prisma/seed.ts",
    "purge": "node --loader ts-node/esm ./prisma/purge.ts"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/pg": "^8.15.5",
    "drizzle-kit": "^0.31.4",
    "prisma": "^6.13.0",
    "supertest": "^7.0.0"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@oslojs/crypto": "^1.0.1",
    "@oslojs/encoding": "^1.1.0",
    "@oslojs/otp": "^1.0.0",
    "@prisma/client": "^6.4.0",
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/method-override": "^0.0.35",
    "@types/node": "^22.9.3",
    "@types/supertest": "^6.0.2",
    "@useoptic/express-middleware": "^0.0.5",
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.44.5",
    "eslint": "^9.20.1",
    "express": "^5.1.0",
    "globals": "^16.0.0",
    "http-errors": "^2.0.0",
    "http-status-codes": "^2.3.0",
    "husky": "^9.1.7",
    "method-override": "^3.0.0",
    "nodemon": "^3.1.7",
    "pg": "^8.16.3",
    "redis": "^4.7.0",
    "reflect-metadata": "^0.2.2",
    "socket.io": "^4.8.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.3",
    "vitest": "^3.0.6",
    "winston": "^3.17.0",
    "zod": "^4.0.0"
  }
}